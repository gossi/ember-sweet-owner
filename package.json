{
  "name": "ember-sweet-owner",
  "version": "1.0.1",
  "description": "Read API for Ember's owner",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Thomas Gossmann",
  "repository": "https://github.com/gossi/ember-sweet-owner",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "imports": {
    "#src": "./src/index.ts"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist",
    "declarations"
  ],
  "scripts": {
    "build": "rollup --config",
    "dev": "vite dev",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "prepack": "pnpm build",
    "release": "release-it",
    "start": "rollup --config --watch",
    "test": "vite build --mode=development --out-dir dist-tests && testem --file testem.cjs ci --port 0"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.10.0",
    "@embroider/macros": "^1.18.0",
    "inflection": "^3.0.0"
  },
  "peerDependencies": {
    "ember-polaris-service": "^0.5.0"
  },
  "peerDependenciesMeta": {
    "ember-polaris-service": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/library-tsconfig": "^1.1.3",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/addon-dev": "^8.1.2",
    "@embroider/compat": "^4.1.11",
    "@embroider/core": "^4.4.0",
    "@embroider/vite": "^1.4.4",
    "@glint/core": "^2.0.0-alpha.10",
    "@glint/environment-ember-loose": "^2.0.0-alpha.3",
    "@glint/template": "^1.7.3",
    "@gossi/config-eslint": "^1.3.1",
    "@gossi/config-prettier": "^1.1.1",
    "@gossi/config-targets": "^1.1.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/qunit": "^2.19.13",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "ember-polaris-service": "^0.5.0",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.9.0",
    "eslint": "^9.39.1",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-qunit": "^8.2.5",
    "prettier": "^3.7.4",
    "qunit": "^2.24.3",
    "release-plan": "^0.17.2",
    "rollup": "^4.53.3",
    "testem": "^3.16.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  },
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": "^20.11.0 || >=22"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs"
  }
}
